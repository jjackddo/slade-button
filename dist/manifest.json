{
  "manifest_version": 3,
  "name": "Confluence Gemini Assistant",
  "version": "1.0.0",
  "description": "Confluence 페이지에 AI 어시스턴트를 추가합니다.",
  "permissions": [
    "sidePanel",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*.atlassian.net/*",
    "https://gemini.google.com/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-LOeicCtx.js"
      ],
      "matches": [
        "https://*.atlassian.net/wiki/spaces/*"
      ],
      "css": [
        "assets/index-CV-HsBZ-.css"
      ]
    }
  ],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "action": {
    "default_title": "Open Gemini Assistant"
  },
  "web_accessible_resources": [
    {
      "matches": [
        "https://*.atlassian.net/*"
      ],
      "resources": [
        "assets/index.tsx-LOeicCtx.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
